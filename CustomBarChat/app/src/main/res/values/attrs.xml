<?xml version="1.0" encoding="utf-8"?>
<resources>

    <!--花式进度条-->
    <declare-styleable name="SectionProgressBar">
        <attr name="section_background" format="color" />
        <attr name="section_foreground" format="color" />
        <attr name="section_space_color" format="color" />
        <attr name="section_text_size" format="dimension" />
        <attr name="section_text_color" format="color" />
        <attr name="section_light_text_color" format="color" />
        <attr name="section_bar_height" format="dimension" />
        <attr name="section_bar_cursor" format="reference" />
        <attr name="section_text_bg_color" format="color" />
        <attr name="section_text_bg_cursor" format="reference" />
    </declare-styleable>

    <declare-styleable name="ScrollBarPic">
        <attr name="scroll_bar_cursor" format="reference" />

    </declare-styleable>
</resources>
